<script>
    var ethReddit = "https://www.reddit.com/r/ethtrader/comments/.json";
    var btcReddit = "https://www.reddit.com/r/BitcoinMarkets/comments/.json";
    var ltcReddit = "https://www.reddit.com/r/LitecoinMarkets/comments/.json";
    function fetchBtc() {
        $.ajax({
            url: btcReddit,
            method: "GET"
        })
            .done(function (response) {
                for (var i = 0; i < 10; i++) {

                    btcInfo = (response.data.children[i].data.body)
                    console.log(response.data.children[i].data.body)

                }
            });
    }

    function fetchEth() {
        $.ajax({
            url: ethReddit,
            method: "GET"
        })
            .done(function (response) {
                for (var i = 0; i < 10; i++) {

                    ethInfo = (response.data.children[i].data.body)
                    console.log(response.data.children[i].data.body)

                }
            });
    }

    function fetchLtc() {
        $.ajax({
            url: ltcReddit,
            method: "GET"
        })
            .done(function (response) {
                for (var i = 0; i < 10; i++) {

                    ltcInfo = (response.data.children[i].data.body)
                    console.log(response.data.children[i].data.body)

                }
            })
    }

    fetchBtc();
    fetchEth();
    fetchLtc();
</script>

